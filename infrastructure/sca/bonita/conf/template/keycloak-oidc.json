  "token-url": "http://keycloak:8080/realms/{realm-name}/protocol/openid-connect/token",
  "logout-url": "{auth-server-url}/realms/{realm-name}/protocol/openid-connect/logout",
  "token-issuer-uri": "{auth-server-url}/realms/{realm-name}",
  "user-info-url": "http://keycloak:8080/realms/{realm-name}/protocol/openid-connect/userinfo",
  "jwks-url": "http://keycloak:8080/realms/{realm-name}/protocol/openid-connect/certs",
